{
	"name": "priorist/edm-typo3",
	"description": "Provides a plugin to render events from EDM in TYPO3.",
	"type": "typo3-cms-extension",
	"config": {
		"platform": {
			"php": "7.4.1"
		}
	},
	"require": {
		"php": "^7.4",
		"priorist/edm-sdk": "^0.6.0",
		"typo3/cms-core": "^10.4 || 11.5.1",
		"typo3/cms-backend": "^10.4 || 11.5.1"
	},
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "priorist GmbH",
			"email": "contact@priorist.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Priorist\\Edm\\": "Classes/"
		}
	},
	"replace": {
		"priorist/edm": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "edm"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org/"
		}
	]
}