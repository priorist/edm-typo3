{
	"name": "priorist/edm-typo3",
	"description": "Provides a plugin to render events from EDM in TYPO3.",
	"type": "typo3-cms-extension",
	"version": "2.0.0",
	"require": {
		"priorist/edm-sdk": "^1.0"
	},
	"license": "GPL-2.0-or-later",
	"authors": [
		{
			"name": "priorist GmbH",
			"email": "contact@priorist.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Priorist\\EdmTypo3\\": "Classes/"
		}
	},
	"replace": {
		"priorist/edm-typo3": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "edm-typo3"
		}
	}
}